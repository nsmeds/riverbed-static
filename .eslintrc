{
    "parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module",
		"ecmaFeatures": {
			"modules": true,
			"experimentalObjectRestSpread": true,
			"impliedStrict": true
		}
	},
    "env": {
        "es6": true,
        "node": true,
        "mocha": true,
        "protractor": true,
        "jasmine": true
    },
    "globals": {
        "angular": false,
        "chai": false
    },
    "extends": "eslint:recommended",
    "rules": {
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-console": 0,
        "no-unused-vars": 0
    }
}
